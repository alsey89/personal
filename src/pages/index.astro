---
import BaseLayout from "@/layouts/BaseLayout.astro";
import {
  Card,
  CardContent,
  CardDescription,
  CardTitle,
  CardHeader,
  CardFooter,
} from "@/components/ui/card";
import {
  GoIcon,
  NodeIcon,
  NuxtIcon,
  VueIcon,
  PostgresIcon,
} from "@/components/icon";
---

<BaseLayout title="Home">
  <main>
    <div class="w-full min-h-screen flex justify-center items-center">
      <div class="w-full h-full flex flex-col justify-center items-center">
        <Card class="w-1/2 h-1/2 md:w-1/4 md:h-1/4">
          <CardTitle class="text-center text-wrap">COMING SOON!</CardTitle>
        </Card>
      </div>
    </div>
  </main>
</BaseLayout>
